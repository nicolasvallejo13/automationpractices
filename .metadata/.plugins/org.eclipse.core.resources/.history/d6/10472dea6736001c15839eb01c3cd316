
public class palabra {
	String nombre;
	String tipo;
	
	
	public palabra ( String nombre, String tipo)
	{
		this.nombre = nombre;
		this.tipo = tipo;
		
	}


	public String getNombre() {
		return nombre;
	}


	public void setNombre(String nombre) {
		this.nombre = nombre;
	}


	public String getTipo() {
		return tipo;
	}


	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	public void guardarPalabra ()
	{
		escribir objeto = new escribir(nombre);
		
	}

}
